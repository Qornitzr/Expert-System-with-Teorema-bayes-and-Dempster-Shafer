<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
    android:id="@+id/cardView_dataPengetahuanList"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginHorizontal="10dp"
    android:layout_marginTop="10dp"
    android:layout_marginBottom="10dp"
    android:scrollbars="vertical"
    android:background="#E8E8E8"
    app:cardCornerRadius="30dp">

    <RelativeLayout
        android:id="@+id/relativeView1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/textView1"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:textColor="@color/colorPrimary"
            android:text="ID"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textViewtitik1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text=":"
            android:textStyle="bold"
            android:textColor="@color/colorPrimary"
            android:layout_toEndOf="@+id/textView1"/>

        <TextView
            android:id="@+id/tv_IdPengetahuan"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="16dp"
            android:textColor="@color/colorPrimary"
            android:text="TextView"
            android:layout_toEndOf="@+id/textViewtitik1"/>

        <TextView
            android:id="@+id/textView2"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:text="Nama Penyakit"
            android:textStyle="bold"
            android:textColor="@color/colorPrimary"
            android:layout_below="@+id/textView1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textViewtitik2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text=":"
            android:textStyle="bold"
            android:textColor="@color/colorPrimary"
            android:layout_below="@+id/textViewtitik1"
            android:layout_toEndOf="@+id/textView2"/>

        <TextView
            android:id="@+id/tv_NamaPenyakitPeng"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/tv_IdPengetahuan"
            android:layout_marginStart="5dp"
            android:layout_marginEnd="16dp"
            android:layout_toEndOf="@+id/textViewtitik2"
            android:text="TextView"
            android:textColor="@color/colorPrimary" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:layout_marginStart="16dp"
            android:textColor="@color/colorPrimary"
            android:text="Nama Gejala"
            android:textStyle="bold"
            android:layout_below="@+id/tv_NamaPenyakitPeng"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textViewtitik3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:textColor="@color/colorPrimary"
            android:text=":"
            android:textStyle="bold"
            android:layout_below="@+id/tv_NamaPenyakitPeng"
            android:layout_toEndOf="@+id/textView2"/>

        <TextView
            android:id="@+id/tv_NamaGejalaPeng"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:textColor="@color/colorPrimary"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp"
            android:text="@string/Lorem"
            android:layout_below="@+id/tv_NamaPenyakitPeng"
            android:layout_toEndOf="@+id/textViewtitik2"/>

        <TextView
            android:id="@+id/textView4"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:layout_marginStart="16dp"
            android:textColor="@color/colorPrimary"
            android:text="Nilai MB"
            android:textStyle="bold"
            android:layout_below="@+id/tv_NamaGejalaPeng"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:visibility="visible"/>

        <TextView
            android:id="@+id/textViewtitik4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:textColor="@color/colorPrimary"
            android:text=":"
            android:textStyle="bold"
            android:layout_below="@+id/tv_NamaGejalaPeng"
            android:layout_toEndOf="@+id/textView3"
            android:visibility="visible"/>

        <TextView
            android:id="@+id/tv_ProbabilitasMB"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:textColor="@color/colorPrimary"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp"
            android:text="TextView"
            android:layout_below="@+id/tv_NamaGejalaPeng"
            android:layout_toEndOf="@+id/textViewtitik3"
            android:visibility="visible"/>

        <TextView
            android:id="@+id/textView5"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:layout_marginStart="16dp"
            android:textColor="@color/colorPrimary"
            android:text="Nilai MD"
            android:textStyle="bold"
            android:layout_below="@+id/tv_ProbabilitasMB"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:visibility="visible"/>

        <TextView
            android:id="@+id/textViewtitik5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:textColor="@color/colorPrimary"
            android:text=":"
            android:textStyle="bold"
            android:layout_below="@+id/tv_ProbabilitasMB"
            android:layout_toEndOf="@+id/textView4"
            android:visibility="visible"/>

        <TextView
            android:id="@+id/tv_ProbabilitasMD"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:textColor="@color/colorPrimary"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="16dp"
            android:text="TextView"
            android:layout_below="@+id/tv_ProbabilitasMB"
            android:layout_toEndOf="@+id/textViewtitik4"
            android:visibility="visible"/>

        <TextView
            android:id="@+id/tv_ProbabilitasTotal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:textColor="@color/colorPrimary"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="16dp"
            android:text="TextView"
            android:layout_below="@+id/tv_ProbabilitasMD"
            android:layout_toEndOf="@+id/textViewtitik4"
            android:visibility="gone"/>

        <TextView
            android:id="@+id/tv_ProbabilitasBayes"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:textColor="@color/colorPrimary"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="16dp"
            android:text="TextView"
            android:layout_below="@+id/tv_ProbabilitasTotal"
            android:layout_toEndOf="@+id/textViewtitik4"
            android:visibility="gone"/>
    </RelativeLayout>


</androidx.cardview.widget.CardView>